{
  "version": 1,
  "projects": {
    "api": {
      "root": "apps/api",
      "sourceRoot": "apps/api/src",
      "projectType": "application",
      "prefix": "api",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@nrwl/node:build",
          "options": {
            "outputPath": "dist/apps/api",
            "main": "apps/api/src/main.ts",
            "tsConfig": "apps/api/tsconfig.app.json",
            "assets": []
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": false,
              "inspect": false
            }
          }
        },
        "serve": {
          "builder": "@nrwl/node:execute",
          "options": {
            "buildTarget": "api:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "apps/api/tsconfig.app.json",
              "apps/api/tsconfig.spec.json"
            ],
            "exclude": ["**/node_modules/**", "!apps/api/**/*"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/api/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "packages-mongo-models": {
      "root": "libs/packages/mongo-models",
      "sourceRoot": "libs/packages/mongo-models/src",
      "projectType": "library",
      "schematics": {},
      "architect": {
        "lint": {
          "builder": "@nrwl/linter:lint",
          "options": {
            "linter": "eslint",
            "tsConfig": [
              "libs/packages/mongo-models/tsconfig.lib.json",
              "libs/packages/mongo-models/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!libs/packages/mongo-models/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/packages/mongo-models/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build-pacakge": {
          "builder": "@nrwl/node:package",
          "options": {
            "outputPath": "dist/libs/packages/mongo-models",
            "tsConfig": "libs/packages/mongo-models/tsconfig.lib.json",
            "packageJson": "libs/packages/mongo-models/package.json",
            "main": "libs/packages/mongo-models/src/index.ts",
            "assets": ["libs/packages/mongo-models/*.md"]
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "api"
}
