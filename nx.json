{
  "npmScope": "pct-org",
  "affected": {
    "defaultBase": "master"
  },
  "implicitDependencies": {
    "workspace.json": "*",
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    "tsconfig.base.json": "*",
    "tslint.json": "*",
    "nx.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "@nrwl/workspace/tasks-runners/default",
      "options": {
        "cacheableOperations": ["build", "lint", "test", "e2e"]
      }
    }
  },
  "workspaceLayout": {
    "libsDir": "internal"
  },
  "projects": {
    "api": {
      "tags": []
    },
    "mongo-models": {
      "tags": ["scope:internal"]
    },
    "package-eztv-api": {
      "tags": ["scope:public"]
    },
    "package-solidtorrents-api": {
      "tags": ["scope:public"]
    },
    "package-pop-api": {
      "tags": ["scope:public"]
    },
    "package-zooqle-api": {
      "tags": ["scope:public"]
    },
    "package-ettv-api": {
      "tags": ["scope:public"]
    },
    "package-kat-api": {
      "tags": ["scope:public"]
    },
    "package-pop-api-scraper": {
      "tags": ["scope:public"]
    }
  }
}
